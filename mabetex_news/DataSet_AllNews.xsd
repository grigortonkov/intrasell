<?xml version="1.0" standalone="yes" ?>
<xs:schema id="DataSet_AllNews" targetNamespace="http://www.tempuri.org/DataSet_AllNews.xsd"
	xmlns:mstns="http://www.tempuri.org/DataSet_AllNews.xsd" xmlns="http://www.tempuri.org/DataSet_AllNews.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata"
	attributeFormDefault="qualified" elementFormDefault="qualified">
	<xs:element name="DataSet_AllNews" msdata:IsDataSet="true" msdata:Locale="de-AT">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="AllNews">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Bezeichnung" type="xs:string" minOccurs="0" />
							<xs:element name="color" type="xs:int" minOccurs="0" />
							<xs:element name="Datum" type="xs:dateTime" minOccurs="0" />
							<xs:element name="ErstelltAm" type="xs:dateTime" minOccurs="0" />
							<xs:element name="ErstelltVon" type="xs:string" minOccurs="0" />
							<xs:element name="KatNr" msdata:AutoIncrement="true" type="xs:int" minOccurs="0" />
							<xs:element name="KurzText" type="xs:string" minOccurs="0" />
							<xs:element name="NewsNr" msdata:AutoIncrement="true" type="xs:int" minOccurs="0" />
							<xs:element name="PrevSeite" type="xs:int" minOccurs="0" />
							<xs:element name="Seiten" type="xs:int" minOccurs="0" />
							<xs:element name="Titel" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>