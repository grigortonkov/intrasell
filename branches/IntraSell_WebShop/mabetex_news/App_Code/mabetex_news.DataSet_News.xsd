<?xml version="1.0" standalone="yes" ?>
<xs:schema id="DataSet_News" targetNamespace="http://www.tempuri.org/DataSet_News.xsd" xmlns:mstns="http://www.tempuri.org/DataSet_News.xsd"
	xmlns="http://www.tempuri.org/DataSet_News.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
	<xs:element name="DataSet_News" msdata:IsDataSet="true" msdata:Locale="de-AT">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="newsPrevNext">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Datum" type="xs:dateTime" minOccurs="0" />
							<xs:element name="ErstelltAm" type="xs:dateTime" minOccurs="0" />
							<xs:element name="ErstelltVon" type="xs:string" minOccurs="0" />
							<xs:element name="KatNr" type="xs:int" minOccurs="0" />
							<xs:element name="KurzText" type="xs:string" minOccurs="0" />
							<xs:element name="NewsNr" msdata:AutoIncrement="true" type="xs:int" />
							<xs:element name="nextNews" msdata:AutoIncrement="true" type="xs:int" minOccurs="0" />
							<xs:element name="nextNewsURL" msdata:ReadOnly="true" type="xs:string" minOccurs="0" />
							<xs:element name="nextNewsURLText" msdata:ReadOnly="true" type="xs:string" minOccurs="0" />
							<xs:element name="Picture" type="xs:string" minOccurs="0" />
							<xs:element name="prevNewsURL" msdata:ReadOnly="true" type="xs:string" minOccurs="0" />
							<xs:element name="prevNewsURLText" msdata:ReadOnly="true" type="xs:string" minOccurs="0" />
							<xs:element name="PrevSeite" type="xs:int" minOccurs="0" />
							<xs:element name="Text" type="xs:string" minOccurs="0" />
							<xs:element name="Titel" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
		<xs:unique name="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:newsPrevNext" />
			<xs:field xpath="mstns:NewsNr" />
		</xs:unique>
	</xs:element>
</xs:schema>